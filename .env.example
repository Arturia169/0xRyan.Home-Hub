# Telegram Bot Token（必填）
TELEGRAM_BOT_TOKEN=your_bot_token_here

# 允许使用的用户 ID（可选，留空允许所有人，逗号分隔多个 ID）
ALLOWED_USER_IDS=

# Arbitrum RPC URL（可选，使用公共 RPC）
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# Etherscan API Key（可选，用于获取交易历史）
ETHERSCAN_API_KEY=your_etherscan_api_key

# TronGrid API Key（可选但推荐）
TRON_API_KEY=your_trongrid_api_key

# 监控间隔（秒）
MONITOR_INTERVAL=30

# 价格更新间隔（秒）
PRICE_UPDATE_INTERVAL=60

# 日志级别
LOG_LEVEL=info

# Telegram Mini App Dashboard URL
WEBAPP_URL=https://your-domain.com

# Cloudflare Tunnel Token（从 Cloudflare Zero Trust 获取）
CF_TUNNEL_TOKEN=your_tunnel_token_here

# API Key 用于前后端通信
HOME_DASHBOARD_API_KEY=your-secure-random-key

# 网络代理（中国境内服务器必填）
HTTPS_PROXY=http://127.0.0.1:7890
